group :development do
  gem 'capistrano',         '~> 3.0.1'
  gem 'capistrano-rvm',     '~> 0.1.0', github: 'capistrano/rvm'
  gem 'capistrano-bundler', '>= 1.1.0'
  gem 'capistrano-rails',   '~> 1.1.0'
  gem 'quiet_assets'
end

gem 'airbrake', '~> 3.1.15'
gem 'httparty'
